#include <stdio.h>
int main(){
    int números [3][4];
    int linhas = sizeof(números)/sizeof(números[0]);
    int colunas = sizeof(números[0])/sizeof(números[0][0]);
    números[0][0]=1;
    números[0][1]=2;
    números[0][2]=3;
    números[0][3]=4;
    números[1][0]=5;
    números[1][1]=6;
    números[1][2]=7;
    números[1][3]=8;
    números[2][0]=9;
    números[2][1]=10;
    números[2][2]=11;
    números[2][3]=12;
    for(int i=0; i < linhas; i++){
        for(int j=0; j < colunas; j++){
            printf("%d ", números[i][j]);
        }
        printf("\n");
    }
    return 0;
}
